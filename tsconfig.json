{
  "compilerOptions": {
    "outDir": "./dist",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "jsx": "react-jsxdev",
    "baseUrl": ".",
    "paths": {
      "@main": [
        "integration/ts/main.ts"
      ]
    },
    "target": "es2016",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "allowJs": false,
    "skipLibCheck": true
  }
}
